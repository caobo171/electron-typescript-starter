{
    "name": "com.selftaught",
    "version": "0.2.4",
    "private": true,
    "main": "main.js",
    "homepage": "./",
    "author": "Selftaught",
    "description": "Selftaught is a platform for every learners",
    "build": {
      "appId": "vn.base.checkin",
      "productName": "Self taught",
      "files": [
        "build/**/*",
        "node_modules/**/*",
        "main/**/*",
        "basevision/**/*",
        "main.js"
      ],
      "directories": {
        "buildResources": "assets"
      },
      "extends": null,
      "publish": {
        "provider": "generic",
        "url": "https://basedesktopapp.s3.ap-southeast-1.amazonaws.com/"
      },
      "protocols": [
        {
          "name": "vn.base.checkin",
          "role": "Viewer",
          "schemes": [
            "vn.base.checkin"
          ]
        }
      ],
      "win": {
        "target": [
          "nsis",
          "appx"
        ]
      },
      "nsis": {
        "perMachine": true
      }
    },
    "dependencies": {
      "app-root-path": "^2.2.1",
      "auto-launch": "^5.0.5",
      "babel-plugin-macros": "^2.5.1",
      "color": "^3.1.2",
      "dexie": "^2.0.4",
      "electron-push-receiver": "^2.1.2",
      "electron-updater": "4.0.6",
      "electron-windows-badge": "^1.0.5"
    },
    "devDependencies": {
      "electron": "4.1.3",
      "electron-builder": "20.39.0"
    },
    "scripts": {
      "postinstall": "electron-builder install-app-deps",
      "electron-dev": "cross-env NODE_ENV=development electron .",
      "electron-prod": "concurrently \"cross-env NODE_ENV=production yarn start\" \"cross-env NODE_ENV=production electron .\"",
      "build-exe": "./scripts/build.sh"
    },
    "eslintConfig": {
      "extends": "react-app"
    },
    "browserslist": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "nodeGypRebuild": true
  }
  